{
  "name": "Save to Scrapbox",
  "icons": {"128": "icon.png"},
  "version": "1.1.1",
  "manifest_version": 2,
  "description": "Save web page to your Scrapbox",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Save this page to your Scrapbox"
  },
  "background": {
    "scripts": ["content.js"],
    "persistent": false
  },
  "options_page": "options.html"
}
